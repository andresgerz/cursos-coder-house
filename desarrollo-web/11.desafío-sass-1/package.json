  },
  "homepage": "https://github.com/andresgerz/consultorio_rx-gerzenstein#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "cross-var": "^1.1.0",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "postcss-cli": "^6.1.3"
  }
}
